{
    "id": "linear-test",
    "name": "Linear Workflow",
    "version": "1.0.0",
    "globalConfig": {
        "isTestnet": true,
        "secrets": [
            { "name": "API_KEY" }
        ],
        "rpcs": [
            { "chainName": "ethereum-mainnet", "url": "https://rpc.example.com/ethereum-mainnet" },
            { "chainName": "ethereum-testnet-sepolia", "url": "https://rpc.example.com/ethereum-testnet-sepolia" }
        ]
    },
    "nodes": [
        {
            "id": "t1",
            "type": "cronTrigger",
            "position": { "x": 0, "y": 0 },
            "data": {
                "label": "Trigger",
                "config": { "schedule": "0 */5 * * * *" }
            }
        },
        {
            "id": "h1",
            "type": "httpRequest",
            "position": { "x": 0, "y": 0 },
            "data": {
                "label": "Fetch Data",
                "config": {
                    "method": "GET",
                    "url": "https://api.example.com/data",
                    "authentication": { "type": "bearerToken", "tokenSecret": "API_KEY" },
                    "responseFormat": "json"
                }
            }
        },
        {
            "id": "p1",
            "type": "jsonParse",
            "position": { "x": 0, "y": 0 },
            "data": {
                "label": "Parse Response",
                "config": {}
            }
        },
        {
            "id": "r1",
            "type": "return",
            "position": { "x": 0, "y": 0 },
            "data": {
                "label": "Done",
                "config": { "returnExpression": "\"ok\"" }
            }
        }
    ],
    "edges": [
        { "id": "e1", "source": "t1", "target": "h1" },
        { "id": "e2", "source": "h1", "target": "p1" },
        { "id": "e3", "source": "p1", "target": "r1" }
    ],
    "createdAt": "2025-01-01T00:00:00Z",
    "updatedAt": "2025-01-01T00:00:00Z"
}
